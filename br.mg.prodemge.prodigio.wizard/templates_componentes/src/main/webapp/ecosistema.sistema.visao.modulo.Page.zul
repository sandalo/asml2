<window id="<%conceito%>_window"
	apply="<%ecosistema_raw_matching%><%ecosistema_name%>.<%sistema_name%>.controle.<%modulo%>.<%Conceito%>Ctr"
	sizable="true" title="<%Conceito%>">
		<div id="barradebotoes" align="right"/>
	
		<selecaobox>
		<sylistbox id="listaSelecao">
			<auxhead class="cabecalhoPesquisa">
				<auxheader>
					<label value="ID:" />
					<intboxarg id="idArg"></intboxarg>
				</auxheader>


			</auxhead>

			<listhead id="cabecalho">
				<listheadersort label="ID" value="id" />
			</listhead>

			<listitem height="25px">
				<listcellbind label="id">
				</listcellbind>
			</listitem>


		</sylistbox>

	</selecaobox>

	<manutencaobox>
		<tablelayout>
			<tablechildren>
				<vbox>
					<label value="ID:" />
					<label value="@{classecontrole.objetoAtual.id}" />
				</vbox>
			</tablechildren>
		</tablelayout>
	</manutencaobox>

</window>